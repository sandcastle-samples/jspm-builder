{
  "name": "jspm-builder",
  "version": "1.0.0",
  "author": "sandcastle",
  "license": "MIT",
  "description": "A sample project for building JSPM 0.17, with typescript",
  "main": "index.js",
  "dependencies": {
    "jquery": "^2.2.3",
    "lodash": "^4.12.0",
    "plugin-typescript": "github:frankwallis/plugin-typescript@^4.0.10"
  },
  "peerDependencies": {
    "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
    "os": "github:jspm/nodelibs-os@^0.2.0-alpha"
  },
  "scripts": {
    "start": "sh -c 'script/server'",
    "build": "sh -c 'script/build'",
    "server": "sh -c 'script/server'",
    "update": "sh -c 'script/update'",
    "test": "echo \"No tests :(\" && exit 1"
  },
  "jspm": {
    "registry": "npm",
    "directories": {
      "baseURL": "src"
    },
    "configFile": "src/jspm.config.js",
    "overrides": {
      "npm:bluebird@3.3.5": {
        "meta": {
          "js/browser/bluebird.js": {
            "format": "global"
          },
          "js/browser/bluebird.min.js": {
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:engine.io@1.6.8": {
        "dependencies": {
          "base64id": "0.1.0",
          "debug": "2.2.0",
          "ws": "1.0.1",
          "engine.io-parser": "1.2.4",
          "accepts": "1.3.1"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.3": {
        "format": "global"
      },
      "npm:readable-stream@1.0.34": {
        "map": {
          "stream": "stream-browserify/index"
        },
        "systemjs": {
          "main": "readable.js"
        },
        "dependencies": {
          "core-util-is": "~1.0.0",
          "isarray": "0.0.1",
          "string_decoder": "~0.10.0",
          "inherits": "~2.0.1",
          "stream-browserify": "~1.0.0"
        }
      },
      "npm:socket.io-client@1.4.5": {
        "main": "lib/index.js"
      },
      "npm:socket.io@1.4.5": {
        "map": {
          "socket.io-client/package": "socket.io-client/package.json"
        }
      },
      "npm:typescript@1.8.10": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      },
      "npm:ws@1.0.1": {
        "dependencies": {
          "options": ">=0.0.5",
          "ultron": "1.0.x",
          "utf-8-validate": "1.2.x",
          "bufferutil": "1.2.x"
        }
      }
    }
  },
  "devDependencies": {
    "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
    "browser-sync": "^2.12.8",
    "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
    "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
    "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
    "dgram": "github:jspm/nodelibs-dgram@^0.2.0-alpha",
    "dns": "github:jspm/nodelibs-dns@^0.2.0-alpha",
    "ecc-jsbn": "~0.1.1",
    "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
    "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
    "fsevents": "^1.0.0",
    "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
    "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
    "jodid25519": "^1.0.0",
    "jsbn": "0.1",
    "jspm": "^0.17.0-beta.14",
    "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
    "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
    "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
    "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
    "punycode": "github:jspm/nodelibs-punycode@^0.2.0-alpha",
    "querystring": "github:jspm/nodelibs-querystring@^0.2.0-alpha",
    "readline": "github:jspm/nodelibs-readline@^0.2.0-alpha",
    "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
    "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
    "tls": "github:jspm/nodelibs-tls@^0.2.0-alpha",
    "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha",
    "tweetnacl": "0.13",
    "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
    "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
    "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
    "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
  }
}
